{
    "parserOptions": {
        "ecmaVersion": 3,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "rules": {
        "quotes": 0,
        "valid-jsdoc": 0,
        "no-var": 0,
        "block-scoped-var": 0,
        "consistent-return": 1,
        "no-redeclare": 1,
        "vars-on-top": 0,
        "no-label-var": 1,
        "no-shadow": 0,
        "no-unused-vars": 1,
        "no-use-before-define": 0,
        "no-mixed-requires": 1,
        "semi": 2,
        "no-console": "off"
    }
}